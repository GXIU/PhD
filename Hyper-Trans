    "def corr(n):\n",
    "    cor = []\n",
    "    for i in range(n):\n",
    "        row = []\n",
    "        for j in range(n):\n",
    "            row.append(np.random.uniform(-1,1))\n",
    "        cor.append(row)\n",
    "    return np.array(cor)"
   ]
